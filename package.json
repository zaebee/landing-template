{
  "name": "landing-template",
  "version": "1.0.0",
  "description": "Template web site with landing page to track telegram bot user source.",
  "scripts": {
    "build": "go run main.go",
    "run": "python -m http.server",
    "test": "echo \"No Go tests implemented yet\"",
    "lint:py": "ruff check . && mypy .",
    "lint:py:fix": "ruff check . --fix && mypy .",
    "format": "prettier --write \"**/*.{json,css,html,js,jsx,ts,tsx,md,mdx}\" && clang-format -i proto/*.proto",
    "lint": "npm run lint:py && npm run format && ruff format",
    "generate-proto:py": "mkdir -p generated && python -m grpc_tools.protoc -I./proto --python_out=./generated --pyi_out=./generated ./proto/*.proto",
    "generate-proto:go": "mkdir -p generated/go && protoc --go_out=./generated/go --go_opt=paths=source_relative -I./proto ./proto/*.proto",
    "generate-proto-win:py": "python -m grpc_tools.protoc -I./proto --python_out=./generated --pyi_out=./generated ./proto/*.proto",
    "generate-proto-win:go": "mkdir -p generated\\go && protoc --go_out=./generated/go --go_opt=paths=source_relative --go-grpc_out=./generated/go --go-grpc_opt=paths=source_relative -I./proto ./proto/*.proto"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "clang-format": "^1.8.0",
    "prettier": "^3.6.2"
  }
}
